{
  "name": "life360",
  "version": "1.7",
  "slug": "life360",
  "description": "Life 360 addon for getting locaiton data",
  "url": "https://github.com/editter/hassio-addons/tree/master/life360",
  "startup": "application",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:8081]",
  "boot": "auto",
  "host_network": false,
  "map": ["ssl"],
  "ports": {
    "8081/tcp": 8081
  },
  "options": {
    "mqttHost": "172.17.0.1",
    "mqttPort": 1883,
    "preface": "life360",
    "mqttUsername": "",
    "mqttPassword": "",
    "ssl": true,
    "certFile": "fullchain.pem",
    "keyFile": "privkey.pem",
    "life360ReturnHost": "",
    "life360User": "",
    "life360Password": "",
    "life360CircleNames": []
  },
  "schema": {
    "mqttHost": "str",
    "mqttPort": "int",
    "preface": "str",
    "mqttUsername": "str",
    "mqttPassword": "str",
    "ssl": "bool",
    "certFile": "str",
    "keyFile": "str",
    "life360ReturnHost": "str",
    "life360User": "str",
    "life360Password": "str",
    "life360CircleNames": ["str"]
  }
}